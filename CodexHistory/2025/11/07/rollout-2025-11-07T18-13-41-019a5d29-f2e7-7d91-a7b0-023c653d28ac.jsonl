                    entry,\n+                                       initial_rsp,\n+                                       stack_base,\n+                                       PROCESS_USER_STACK_SIZE,\n+                                       user_args,\n+                                       3))\n+    {\n+        exec_set_error(error_out, \"thread config failed\");\n+        process_kill(process, -1);\n+        process_destroy(process);\n+        return NULL;\n+    }\n+\n+    if (!process_start_user_process(process))\n+    {\n+        exec_set_error(error_out, \"process start failed\");\n+        process_kill(process, -1);\n+        process_destroy(process);\n+        return NULL;\n+    }\n+\n+    exec_set_error(error_out, NULL);\n+    return process;\n+}\n*** End Patch"}}
