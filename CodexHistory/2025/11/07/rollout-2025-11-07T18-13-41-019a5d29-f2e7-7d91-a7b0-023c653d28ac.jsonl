n+        flags |= PAGE_USER;\n+    }\n+    table[index] = ((uintptr_t)child) | flags;\n+    return child;\n+}\n+\n+bool paging_map_page(paging_space_t *space,\n+                     uintptr_t virt_addr,\n+                     uintptr_t phys_addr,\n+                     bool writable,\n+                     bool executable,\n+                     bool user_accessible)\n+{\n+    if (!space || !space->tables_base)\n+    {\n+        return false;\n+    }\n+\n+    uint64_t *pml4 = (uint64_t *)space->tables_base;\n+    size_t pml4_index = (size_t)((virt_addr >> 39) & 0x1FF);\n+    size_t pdpt_index = (size_t)((virt_addr >> 30) & 0x1FF);\n+    size_t pd_index = (size_t)((virt_addr >> 21) & 0x1FF);\n+    size_t pt_index = (size_t)((virt_addr >> 12) & 0x1FF);\n+\n+    uint64_t *pdpt = paging_ensure_child(space, pml4, pml4_index, user_accessible);\n+    if (!pdpt)\n+    {\n+        return false;\n+    }\n+    uint64_t *pd = paging_ensure_child(space, pdpt, pdpt_index, user_accessible);\n+    if (!pd)\n+    {\n+        return false;\n+    }\n+    uint64_t *pt = paging_ensure_child(space, pd, pd_index, user_accessible);\n+    if (!pt)\n+    {\n+        return false;\n+    }\n+    if (pt[pt_index] & PAGE_PRESENT)\n+    {\n+        return false;\n+    }\n+\n+    uint64_t flags = PAGE_PRESENT;\n+    if (writable)\n+    {\n+        flags |= PAGE_WRITABLE;\n+    }\n+    if (user_accessible)\n+    {\n+        flags |= PAGE_USER;\n+    }\n+    if (g_nx_supported && !executable)\n+    {\n+        flags |= PAGE_NO_EXECUTE;\n+    }\n+    pt[pt_index] = (phys_addr & ~((uintptr_t)PAGE_SIZE_BYTES - 1)) | flags;\n+    return true;\n+}\n+\n+bool paging_map_user_region(paging_space_t *space,\n+                            uintptr_t virt_start,\n+                            size_t length,\n+                            bool writable,\n+                            bool executable)\n+{\n+    if (!space || length == 0)\n+    {\n+        return false;\n+    }\n+\n+    uintptr_t start = align_down(virt_start, PAGE_SIZE_BYTES);\n+    uintptr_t end = align_up(virt_start + length, PAGE_SIZE_BYTES);\n+\n+    for (uintptr_t addr = start; addr < end; addr += PAGE_SIZE_BYTES)\n+    {\n+        void *page = paging_allocate_page(space);\n+        if (!page)\n+        {\n+            return false;\n+        }\n+        if (!paging_map_page(space,\n+                             addr,\n+                             (uintptr_t)page,\n+                             writable,\n+                             executable,\n+                             true))\n+        {\n+            return false;\n+        }\n+    }\n+    return true;\n+}\n*** End Patch"}}
