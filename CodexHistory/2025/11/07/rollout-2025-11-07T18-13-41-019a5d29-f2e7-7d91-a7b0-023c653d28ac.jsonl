    void *context = entry->context;\\n    entry->used = false;\\n    entry->ops = NULL;\\n    entry->context = NULL;\\n\\n    if (ops && ops->close)\\n    {\\n        return ops->close(context);\\n    }\\n    return 0;\\n}\\n\",\"metadata\":{\"exit_code\":0,\"duration_seconds\":0.3}}"}}
