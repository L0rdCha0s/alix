\nchar serial_read_char(void)\\n{\\n    while (!serial_receive_ready())\\n    {\\n    }\\n    return (char)inb(COM1);\\n}\\n\\nbool serial_has_char(void)\\n{\\n    return serial_receive_ready() != 0;\\n}\\n\",\"metadata\":{\"exit_code\":0,\"duration_seconds\":0.2}}"}}
